{
  "npmScope": "zxcvb",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "sample-react-app": {
      "tags": []
    },
    "sample-react-app-e2e": {
      "tags": [],
      "implicitDependencies": [
        "sample-react-app"
      ]
    },
    "ui-shared": {
      "tags": []
    },
    "ui-shared-e2e": {
      "tags": [],
      "implicitDependencies": [
        "ui-shared"
      ]
    }
  }
}
