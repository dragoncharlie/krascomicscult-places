{
  "version": 1,
  "projects": {
    "sample-react-app": {
      "root": "apps/sample-react-app",
      "sourceRoot": "apps/sample-react-app/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/sample-react-app",
            "index": "apps/sample-react-app/src/index.html",
            "main": "apps/sample-react-app/src/main.tsx",
            "polyfills": "apps/sample-react-app/src/polyfills.ts",
            "tsConfig": "apps/sample-react-app/tsconfig.app.json",
            "assets": [
              "apps/sample-react-app/src/favicon.ico",
              "apps/sample-react-app/src/assets"
            ],
            "styles": [
              "apps/sample-react-app/src/styles.css",
              "apps/sample-react-app/src/tailwind.css"
            ],
            "scripts": [],
            "webpackConfig": "react-tailwind.webpack.config.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/sample-react-app/src/environments/environment.ts",
                  "with": "apps/sample-react-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "sample-react-app:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "sample-react-app:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/sample-react-app/tsconfig.app.json",
              "apps/sample-react-app/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/sample-react-app/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/sample-react-app/jest.config.js",
            "tsConfig": "apps/sample-react-app/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "build-tailwind-css": {
          "builder": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "npx tailwindcss-cli@latest build ./src/styles.css -o src/app/tailwind.css",
            "cwd": "apps/sample-react-app"
          }
        }
      }
    },
    "sample-react-app-e2e": {
      "root": "apps/sample-react-app-e2e",
      "sourceRoot": "apps/sample-react-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/sample-react-app-e2e/cypress.json",
            "tsConfig": "apps/sample-react-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "sample-react-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "sample-react-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/sample-react-app-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/sample-react-app-e2e/**/*"
            ]
          }
        }
      }
    },
    "ui-shared": {
      "root": "libs/ui-shared",
      "sourceRoot": "libs/ui-shared/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/ui-shared/tsconfig.lib.json",
              "libs/ui-shared/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/ui-shared/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/ui-shared/jest.config.js",
            "tsConfig": "libs/ui-shared/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/ui-shared/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/ui-shared",
            "config": {
              "configFolder": "libs/ui-shared/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "ui-shared-e2e": {
      "root": "apps/ui-shared-e2e",
      "sourceRoot": "apps/ui-shared-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ui-shared-e2e/cypress.json",
            "tsConfig": "apps/ui-shared-e2e/tsconfig.e2e.json",
            "devServerTarget": "ui-shared:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "ui-shared:storybook:ci"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/ui-shared-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/ui-shared-e2e/**/*"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/react": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "@emotion/styled"
      },
      "library": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "sample-react-app"
}
